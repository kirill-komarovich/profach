                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_history"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.history}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/military_result" />

            <TextView
                android:id="@+id/literature_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_literature"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.literature}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/history_result" />

            <TextView
                android:id="@+id/journalism_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_journalism"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.journalism}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/literature_result" />

            <TextView
                android:id="@+id/social_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_social"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.social}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/journalism_result" />

            <TextView
                android:id="@+id/pedagogy_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_pedagogy"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.pedagogy}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/social_result" />

            <TextView
                android:id="@+id/jurisprudence_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_jurisprudence"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.jurisprudence}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/pedagogy_result" />

            <TextView
                android:id="@+id/service_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_service"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.service}"
                app:layout_constraintStart_toStartOf="parent"
