<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="viewModel"
            type="com.profach.viewmodels.result.HolomstockResultViewModel" />
    </data>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/result_details"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:text="@string/holomstock_result_details"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Body2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/biology_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_biology"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.biology}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/result_details" />

            <TextView
                android:id="@+id/geography_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_geography"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.geography}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/biology_result" />

            <TextView
                android:id="@+id/geology_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_geology"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.geology}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/geography_result" />

            <TextView
                android:id="@+id/medicine_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_medicine"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.medicine}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/geology_result" />

            <TextView
                android:id="@+id/light_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_light"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.light}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/medicine_result" />

            <TextView
                android:id="@+id/physics_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_physics"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.physics}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/light_result" />

            <TextView
                android:id="@+id/chemistry_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_chemistry"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.chemistry}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/physics_result" />

            <TextView
                android:id="@+id/technique_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_technique"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.technique}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/chemistry_result" />

            <TextView
                android:id="@+id/engineering_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_engineering"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
