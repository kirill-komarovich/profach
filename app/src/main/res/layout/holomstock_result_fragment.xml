            <TextView
                android:id="@+id/math_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_math"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.math}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/service_result" />

            <TextView
                android:id="@+id/economics_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_economics"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.economics}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/math_result" />

            <TextView
                android:id="@+id/languages_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_languages"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.languages}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/economics_result" />

            <TextView
                android:id="@+id/art_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_art"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.art}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/languages_result" />

            <TextView
                android:id="@+id/acting_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_acting"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.acting}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/art_result" />

            <TextView
                android:id="@+id/music_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_music"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.music}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/acting_result" />

            <TextView
                android:id="@+id/sport_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:text="@string/holomstock_sport"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.sport}"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/music_result" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</layout>