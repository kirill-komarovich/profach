                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/technique_result" />

            <TextView
                android:id="@+id/metalworking_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_metalworking"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.metalworking}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/engineering_result" />

            <TextView
                android:id="@+id/woodworking_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_woodworking"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.woodworking}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/metalworking_result" />

            <TextView
                android:id="@+id/construction_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_construction"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.construction}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/woodworking_result" />

            <TextView
                android:id="@+id/transport_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_transport"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
                app:answerValue="@{viewModel.transport}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/construction_result" />

            <TextView
                android:id="@+id/aviation_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/holomstock_aviation"
                android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
